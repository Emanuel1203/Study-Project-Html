    if (window.Kustomer) {
      const { isReturnHomeApp, kustomerJWT, firstName, lastName, phone, tasteProfileForDisplay } = __INITIAL_STATE__.tradeUser || {}
      // default position
      const kustomerPosition = {
        alignment: 'right',
        verticalPadding: 20,
        horizontalPadding: 20,
      }
      const isMobile = window.innerWidth < 768

      if (isMobile && isReturnHomeApp) kustomerPosition.verticalPadding = 90

      window.Kustomer.start({
        chatIconPosition: kustomerPosition
      }, () => {
        if (__INITIAL_STATE__.userIsRemembered) {
          window.Kustomer.login({ jwtToken: kustomerJWT }, (response, error) => {
            if (!error) {
              window.Kustomer.describeCustomer({
                customAttributes: {
                  customerRegisteredBool: true,
                  customerSupplierTypeTree: 'customer',
                  customerFirstNameStr: firstName,
                  customerLastNameStr: lastName,
                  customerPhoneStr: phone,
                  customerTasteProfileTxt: tasteProfileForDisplay
                }
              })
            }
          })
