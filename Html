    // this event is triggered by lazy sizes -- add data-bg to an element
    // and this will alllow images set by css background-image to be lazy loaded
    document.addEventListener('lazybeforeunveil', function (e) {
      const bg = e.target.getAttribute('data-bg');

      if (bg) { e.target.style.backgroundImage = 'url(' + bg + ')'; }
    })
  </script>

